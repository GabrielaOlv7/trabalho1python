def imprime():
print("isso é uma função")
imprime()

#com parametro
def imprime(n):
    print(n)

imprime("qualquer coisa!")

#com retorno
def potencia(n):
    return n * n

x = potencia(10)
print(x)
100

#com valor default
def intervalo(inic=1,fim=10):
    for n in range(inic, fim+1):
        print(n)

x = intervalo(0,15)
y = intervalo()
