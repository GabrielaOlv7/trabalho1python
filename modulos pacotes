import statistics 
z = [30,40,50,60]
x = statistics.mean(z)
y = statistics.median(z)
print(x)
print(y)
45
45.0

import statistics  as est
z = [30,40,50,60]
x = est.mean(z)
y = est.median(z)
print(x)
print(y)
45
45.0

from statistics import mean, median
z = [60,70,80,90]
x = mean(z)
y = median(z)
print(x)
print(y)
75
75.0

from statistics import *
z = [70,80,90,100]
x = mean(z)
y = median(z)
print(x)
print(y)
85
85.0
